</head>
<body>
    

    <div class="main">
        <div class="main__left">
            <div id="video-grid">

            </div>
            <div class="left__control">
                <div class="left__control__block">
                    <div onclick="muteUnmute()" class="left__control__button audio__control__button">
                        <i class="fas fa-microphone"></i>
                        <span class="control__audio">Mute</span>
                    </div>
                    <div onclick="playStop()" class="left__control__button video__control__button">
                        <i class="fas fa-video"></i>
                        <span class="control__video">stop Video</span>
                    </div>
                </div>
                <div class="left__control__block">
                    <div class="left__control__button">
                        <i class="fas fa-shield-alt"></i>
                        <span class="control__security">Security</span>
                    </div>
                    <div class="left__control__button">
                        <i class="fas fa-user-friends"></i>
                        <span class="control__participants">Participants</span>
                    </div>
                    <div class="left__control__button">
                        <i class="fas fa-comment-alt"></i>
                        <span class="control__chat">Chat</span>
                    </div>
                </div>
                <div class="left__control__block">
                    <div class="left__control__button">
                        <span class="control__leaveMeeting">Leave meeting</span>
                    </div>
                </div>
            </div>
        </div>
        <div class="main__right">
            <div class="right__header">
                <h3>chat</h3>
            </div>
            <hr/>
            <div class="right__messages">
                    <ul class="message_list">

                    </ul>
            </div>
            <hr>/
            <div class="main__chat__input">
                <input type="text" placeholder="Type your message here..."/>
            </div>
        </div>
    </div>
  
